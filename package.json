{
  "scripts": {
    "start": "wrangler dev",
    "wrangler:publish": "wrangler deploy"
  },
  "devDependencies": {
    "wrangler": "^4.0.0"
  },
  "template": {
    "mergers": [
      {
        "type": "TOML",
        "pattern": "wrangler.toml",
        "options": {
          "messagePrefix": "chore(wrangler): ",
          "expand": true,
          "mergeHints": {
            "*": {
              "overwrite": false
            }
          }
        }
      }
    ],
    "usedBy": [
      "arlac77/worker-auth-provider",
      "konsumation/cloudflare-worker"
    ]
  }
}
