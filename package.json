{
  "scripts": {
    "start": "wrangler dev",
    "publish": "--overwrite-- wrangler deploy"
  },
  "devDependencies": {
    "wrangler": "^3.7.0"
  },
  "template": {
    "mergers": [
      {
        "type": "TOML",
        "pattern": "wrangler.toml",
        "options": {
          "messagePrefix": "chore(wrangler): ",
          "expand": true,
          "mergeHints": {
            "*": {
              "overwrite": false
            }
          }
        }
      }
    ]
  }
}
